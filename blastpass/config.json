{
    "service_name": "blastpass",
    "vulnbox_directory": "{service_name}",
    "vulnbox": {
        "files": [
            "Dockerfile",
            "docker-compose.yml",
            "src/",
            "deploy.sh",
            "run.sh",
            "README.md"
        ],
        "exclude": [
            "*.pyc",
            "__pycache__/",
            ".git/",
            "*.log",
            ".DS_Store",
            "*.swp",
            "*.tmp",
            ".vscode/",
            ".idea/"
        ]
    },
    "checker": {
        "files": [
            "checker/",
            "checker.sh"
        ],
        "exclude": [
            "*.pyc",
            "__pycache__/",
            ".git/",
            "*.log",
            ".DS_Store",
            "*.swp",
            "*.tmp",
            ".vscode/",
            ".idea/"
        ]
    }
}
